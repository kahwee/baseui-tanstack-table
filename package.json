{
  "name": "baseui-data-table",
  "version": "1.0.1",
  "description": "A data table component for BaseUI using TanStack Table",
  "type": "module",
  "main": "dist/baseui-data-table.umd.js",
  "module": "dist/baseui-data-table.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "storybook:clean": "storybook dev -p 6006 --no-cache",
    "build-storybook": "storybook build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kahwee/baseui-data-table.git"
  },
  "keywords": [
    "baseui",
    "react",
    "table",
    "data-table",
    "tanstack-table"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kahwee/baseui-data-table/issues"
  },
  "homepage": "https://github.com/kahwee/baseui-data-table#readme",
  "dependencies": {
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-table": "^8.21.3",
    "baseui": "^15.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "styletron-engine-atomic": "^1.6.2",
    "styletron-react": "^6.1.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@storybook/addon-docs": "^10.0.6",
    "@storybook/react-vite": "^10.0.6",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react-swc": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "eslint": "^9.39.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "10.0.6",
    "jiti": "^2.6.1",
    "jsdom": "^27.1.0",
    "storybook": "^10.0.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.8"
  },
  "overrides": {
    "@types/react": "$@types/react",
    "@types/react-dom": "$@types/react-dom"
  }
}
